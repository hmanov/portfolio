{
  "kind": "collectionType",
  "collectionName": "blogposts",
  "info": {
    "singularName": "blogpost",
    "pluralName": "blogposts",
    "displayName": "blogpost",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "postImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "Seo": {
      "displayName": "SEO",
      "type": "component",
      "repeatable": true,
      "component": "shared.seo"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Javascript",
        "React",
        "NextJs"
      ]
    }
  }
}
